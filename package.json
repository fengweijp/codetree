{
  "name": "antlr4-ast",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Francois Wouts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -e json,ts,ast --exec ./node_modules/.bin/ts-node -- ./src/dev.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts' -e 'src/parsers/**/*.ts'",
    "validate": "npm ls",
    "generate:Ast": "antlr4ts -no-listener -no-visitor src/parsers/ast/Ast.g4",
    "generate:Java8": "antlr4ts -no-listener -no-visitor src/parsers/examples/java8/Java8.g4"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@types/node": "^7.0.14",
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "nodemon": "^1.11.0",
    "precommit-hook": "^3.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "antlr4ts": "^0.4.0-alpha.4"
  }
}
